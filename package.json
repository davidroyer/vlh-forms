{
  "name": "vlh-forms",
  "version": "0.0.34",
  "description": "Forms Plugin for Learning House's new form system",
  "repository": {
    "url": "davidroyer/vlh-forms",
    "type": "git"
  },
  "main": "dist/vlh-forms.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "test-vue-app",
    "dev": "poi example/index.js",
    "build": "poi build  ./src/index.js --component VlhForms --no-clear",
    "build:example": "poi build example/index.js",
    "prod": "bili --input ./src/index.js --format cjs --format umd --module-name VlhForms --compress umd",
    "deploy": "npm publish",
    "docs": "docsify serve ./docs"
  },
  "author": {
    "name": "David Royer",
    "email": "droyer01@gmail.com"
  },
  "license": "MIT",
  "bili": {
    "format": [
      "cjs",
      "umd"
    ]
  },
  "devDependencies": {
    "bili": "^0.18.2",
    "node-sass": "^4.7.2",
    "poi": "^9.5.5",
    "rollup-plugin-vue": "^2.5.2",
    "sass-loader": "^6.0.6",
    "test-vue-app": "^1.0.0",
    "vue-test-utils": "^1.0.0-beta.2"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "vee-validate": "^2.0.0-rc.21",
    "vue": "^2.5.3"
  }
}
